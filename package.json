{
  "name": "electronics-store-app",
  "version": "1.0.0",
  "description": "Electronics Store Application with React Frontend and Spring Boot Backend",
  "scripts": {
    "install-all": "npm run install-frontend && npm run install-backend",
    "install-frontend": "cd app_frontend && npm install",
    "install-backend": "cd app_backend/springapp && mvn clean install -DskipTests",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-frontend": "cd app_frontend && npm start",
    "start-backend": "cd app_backend/springapp && mvn spring-boot:run",
    "build": "npm run build-frontend && npm run build-backend",
    "build-frontend": "cd app_frontend && npm run build",
    "build-backend": "cd app_backend/springapp && mvn clean package -DskipTests",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "setup": "npm install && npm run install-all"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["react", "spring-boot", "electronics", "store"],
  "author": "",
  "license": "ISC"
}